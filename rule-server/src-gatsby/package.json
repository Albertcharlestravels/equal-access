{
  "name": "gatsby-starter-carbon-theme",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "scripts": {
    "build": "npx copyfiles ./src/gatsby-theme-carbon/config/lunr-options.js ./node_modules/gatsby-theme-carbon/config/lunr-options.js && gatsby clean && gatsby build --prefix-paths",
    "start": "npx copyfiles ./src/gatsby-theme-carbon/config/lunr-options.js ./node_modules/gatsby-theme-carbon/config/lunr-options.js && gatsby develop",
    "start:clean": "gatsby clean && gatsby develop",
    "serve": "npx copyfiles ./src/gatsby-theme-carbon/config/lunr-options.js ./node_modules/gatsby-theme-carbon/config/lunr-options.js && gatsby serve"
  },
  "dependencies": {
    "carbon-icons": "^7.0.7",
    "gatsby": "^2.20.3",
    "gatsby-theme-carbon": "^1.22.1",
    "gatsby-transformer-remark": "^2.7.1",
    "jszip": "^3.2.2",
    "num-to-words": "^0.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "copyfiles": "^2.3.0"
  }
}
