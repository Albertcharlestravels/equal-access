    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Sample Server",
            "program": "${workspaceFolder}/TestFileServerLocal",
            "cwd": "${workspaceRoot}",
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Selenium Server",
            "program": "${workspaceFolder}/node_modules/protractor/bin/webdriver-manager",
            "args": [ "start" ],
            "cwd": "${workspaceRoot}",
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Test",
            "program": "${workspaceFolder}/node_modules/protractor/bin/protractor",
            "args": ["conf.js"],
            "cwd": "${workspaceRoot}",
            "console": "integratedTerminal"
        }
    ]
}